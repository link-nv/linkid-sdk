                        <documentation>Optional date filter</documentation>
                    </annotation>
                </element>
                <element name="pageFilter" type="tns:ReportPageFilter" minOccurs="0" maxOccurs="1">
                    <annotation>
                        <documentation>Optional page filter</documentation>
                    </annotation>
                </element>
            </sequence>
            <attribute name="language" type="string" use="optional">
                <annotation>
                    <documentation>ISO 639 alpha-2 or alpha-3 language code. Optional, default is en</documentation>
                </annotation>
            </attribute>
        </complexType>
    </element>

    <element name="VoucherListRedeemedResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:VoucherListRedeemedSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:VoucherListRedeemedError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="VoucherListRedeemedSuccess">
        <sequence>
            <element name="vouchers" type="tns:Voucher" minOccurs="0" maxOccurs="unbounded" />
        </sequence>
        <attribute name="total" type="long" use="required" />
    </complexType>

    <complexType name="VoucherListRedeemedError">
		<attribute name="errorCode" type="tns:VoucherListRedeemedErrorCode" use="required" />
    </complexType>

    <simpleType name="VoucherListRedeemedErrorCode">
		<annotation>
			<documentation>Voucher List Redeemed Error codes</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.unknown.user">
				<annotation>
					<documentation>No such user</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.unknown.voucher.organization">
				<annotation>
					<documentation>No such voucher organization</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.too.many.results">
				<annotation>
					<documentation>There are too many results to return, please narrow down your search</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.invalid.page">
				<annotation>
					<documentation>Invalid page filter config, e.g. firstResult > total</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.permission.denied">
				<annotation>
					<documentation>Application not allowed to list vouchers for this organization</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.unexpected">
				<annotation>
					<documentation>Something unexpected happened</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- Vouchers: voucher info -->

