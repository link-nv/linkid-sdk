		<annotation>
			<documentation>Voucher Info Error codes</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.unknown.voucher.id">
				<annotation>
					<documentation>No such voucher ID exists</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.permission.denied">
				<annotation>
					<documentation>Application not allowed to access voucher info for requested voucher</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.unexpected">
				<annotation>
					<documentation>Something unexpected happened</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- Vouchers: redeem -->

    <element name="VoucherRedeemRequest">
        <complexType>
            <sequence>
                <element name="voucherId" type="string" minOccurs="1" maxOccurs="1">
                    <annotation>
                        <documentation>The ID of the voucher to redeem</documentation>
                    </annotation>
                </element>
            </sequence>
        </complexType>
    </element>

    <element name="VoucherRedeemResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:VoucherRedeemSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:VoucherRedeemError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="VoucherRedeemSuccess">
    </complexType>

    <complexType name="VoucherRedeemError">
		<attribute name="errorCode" type="tns:VoucherRedeemErrorCode" use="required" />
    </complexType>

    <simpleType name="VoucherRedeemErrorCode">
		<annotation>
			<documentation>Voucher Redeem Error codes</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.unknown.voucher">
				<annotation>
					<documentation>No such voucher with that ID</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.already.redeemed">
				<annotation>
					<documentation>Specified voucher was already redeemed</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.permission.denied">
				<annotation>
					<documentation>Application not allowed to redeem vouchers for this organization</documentation>
				</annotation>
