    <complexType name="VoucherOrganizationHistoryError">
        <sequence>
            <element name="errorMessage" type="string" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>Optional error message containing additional info</documentation>
                </annotation>
            </element>
        </sequence>
		<attribute name="errorCode" type="tns:VoucherOrganizationHistoryErrorCode" use="required" />
    </complexType>

    <simpleType name="VoucherOrganizationHistoryErrorCode">
		<annotation>
			<documentation>Voucher organization activate error codes</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.unknown.voucher.organization">
				<annotation>
					<documentation>No such voucher organization</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.too.many.results">
				<annotation>
					<documentation>There are too many results to return, please narrow down your search</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.invalid.page">
				<annotation>
					<documentation>Invalid page filter config, e.g. firstResult > total</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.permission.denied">
				<annotation>
					<documentation>Not owner, ...</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.unexpected">
				<annotation>
					<documentation>Something unexpected happened</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- Voucher: common -->

    <complexType name="VoucherOrganizationDetails">
        <sequence>
            <element name="organization" type="tns:VoucherOrganization" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Voucher organization configuration</documentation>
                </annotation>
            </element>
            <element name="stats" type="tns:VoucherOrganizationStats" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>Optional voucher organization stats, if requested</documentation>
                </annotation>
            </element>
            <element name="rewardPermissionApplications" type="string" minOccurs="0" maxOccurs="unbounded">
                <annotation>
                    <documentation>List of applications that have the REWARD permission</documentation>
                </annotation>
            </element>
            <element name="listPermissionApplications" type="string" minOccurs="0" maxOccurs="unbounded">
                <annotation>
                    <documentation>List of applications that have the LIST permission</documentation>
                </annotation>
            </element>
            <element name="redeemPermissionApplications" type="string" minOccurs="0" maxOccurs="unbounded">
                <annotation>
                    <documentation>List of applications that have the REDEEM permission</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="VoucherOrganization">
        <sequence>
            <element name="voucherOrganizationId" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The ID of the voucher organization to add/update</documentation>
                </annotation>
            </element>
            <element name="logoUrl" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The url of the voucher logo</documentation>
                </annotation>
            </element>
            <element name="voucherLimit" type="long" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The size of the voucher</documentation>
                </annotation>
            </element>
            <element name="active" type="boolean" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Is the voucher organization active?</documentation>
                </annotation>
            </element>
            <element name="nameLocalization" type="tns:Localization" minOccurs="1" maxOccurs="unbounded">
                <annotation>
                    <documentation>The voucher organization name localizations</documentation>
                </annotation>
            </element>
            <element name="descriptionLocalization" type="tns:Localization" minOccurs="1" maxOccurs="unbounded">
                <annotation>
                    <documentation>The voucher organization description localizations</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="VoucherOrganizationStats">
        <sequence>
            <element name="numberOfVouchers" type="long" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Total number of vouchers, not yet active, active and redeemed ones</documentation>
                </annotation>
            </element>
            <element name="numberOfInactiveVouchers" type="long" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Total number of inactive vouchers, i.e. voucher limit not yet reached</documentation>
                </annotation>
            </element>
            <element name="numberOfActiveVouchers" type="long" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Total number of active vouchers, i.e. not yet redeemed</documentation>
                </annotation>
            </element>
            <element name="numberOfRedeemedVouchers" type="long" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Total number of redeemed vouchers</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="Voucher">
        <sequence>
            <element name="id" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The ID of the specific voucher</documentation>
                </annotation>
            </element>
            <element name="name" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Localized name of the voucher organization</documentation>
                </annotation>
            </element>
            <element name="description" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Localized description of the voucher organization</documentation>
                </annotation>
            </element>
            <element name="logoUrl" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>URL of the logo of the voucher organization</documentation>
                </annotation>
            </element>
            <element name="counter" type="long" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Voucher counter</documentation>
                </annotation>
            </element>
            <element name="limit" type="long" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Voucher limit</documentation>
                </annotation>
            </element>
            <element name="activated" type="dateTime" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>Time the voucher was activated ( i.e. # points on it > voucher.organization.limit )</documentation>
                </annotation>
            </element>
            <element name="redeemed" type="dateTime" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>Time the voucher was redeemed</documentation>
                </annotation>
            </element>
            <element name="voucherOrganizationId" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The voucher's organization ID</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="VoucherHistoryEvent">
        <sequence>
            <element name="id" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>History event ID</documentation>
                </annotation>
            </element>
            <element name="date" minOccurs="1" maxOccurs="1" type="dateTime">
                <annotation>
                    <documentation>When did the event occur</documentation>
                </annotation>
            </element>
            <element name="voucherOrganizationId" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The voucher's organization ID</documentation>
                </annotation>
            </element>
            <element name="userId" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The user ID</documentation>
                </annotation>
            </element>
            <element name="voucherId" type="string" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>The ID of the specific voucher if applicable</documentation>
                </annotation>
            </element>
            <element name="points" type="long" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Voucher points in event (if applicable)</documentation>
                </annotation>
            </element>
            <element name="applicationName" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The id of the application responsible for this event</documentation>
                </annotation>
            </element>
            <element name="applicationNameFriendly" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The friendly name of the application responsible for this event</documentation>
                </annotation>
            </element>
            <element name="eventType" type="tns:VoucherHistoryEventType" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The event type</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <simpleType name="VoucherHistoryEventType">
        <restriction base="string">
            <enumeration value="voucher.event.reward" />
            <enumeration value="voucher.event.activate" />
            <enumeration value="voucher.event.redeem" />
        </restriction>
    </simpleType>

</schema>