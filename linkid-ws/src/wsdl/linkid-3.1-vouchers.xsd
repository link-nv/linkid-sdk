    <element name="VoucherInfoResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:VoucherInfoSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:VoucherInfoError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="VoucherInfoSuccess">
        <sequence>
            <element name="voucher" type="tns:Voucher" minOccurs="0" maxOccurs="1">
                <annotation>
                    The voucher info
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="VoucherInfoError">
		<attribute name="errorCode" type="tns:VoucherInfoErrorCode" use="required" />
