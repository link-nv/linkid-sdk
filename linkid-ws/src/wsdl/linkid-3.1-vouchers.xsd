<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:net:lin-k:linkid:3.1:core" xmlns:tns="urn:net:lin-k:linkid:3.1:core"
        elementFormDefault="qualified">

    <include schemaLocation="linkid-3.1-common.xsd" />

    <!-- **************************************************************************************************************************************************
         *                                                                  Vouchers                                                                      *
         **************************************************************************************************************************************************  -->

    <!-- Vouchers: reward -->

    <element name="VoucherRewardRequest">
        <complexType>
            <sequence>
                <element name="userId" type="string" minOccurs="1" maxOccurs="1">
                    <annotation>
                        <documentation>User ID to give voucher points to</documentation>
                    </annotation>
                </element>
                <element name="voucherOrganizationId" type="string" minOccurs="1" maxOccurs="1">
                    <annotation>
                        <documentation>The voucher organization ID</documentation>
                    </annotation>
                </element>
                <element name="points" type="long" minOccurs="1" maxOccurs="1">
                    <annotation>
                        <documentation>Amount of points to reward</documentation>
                    </annotation>
                </element>
            </sequence>
        </complexType>
    </element>

    <element name="VoucherRewardResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:VoucherRewardSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:VoucherRewardError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="VoucherRewardSuccess">
    </complexType>

    <complexType name="VoucherRewardError">
		<attribute name="errorCode" type="tns:VoucherRewardErrorCode" use="required" />
    </complexType>

    <simpleType name="VoucherRewardErrorCode">
		<annotation>
			<documentation>Voucher Reward Error codes</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.unknown.user">
				<annotation>
					<documentation>No such user</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.inactive.voucher">
				<annotation>
					<documentation>Voucher is inactive</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.unknown.voucher.organization">
				<annotation>
					<documentation>No such voucher organization</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.permission.denied">
				<annotation>
					<documentation>Application not allowed to modify vouchers for this organization</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.unexpected">
				<annotation>
					<documentation>Something unexpected happened</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- Vouchers: list -->

    <element name="VoucherListRequest">
        <complexType>
            <sequence>
                <element name="userId" type="string" minOccurs="1" maxOccurs="1">
                    <annotation>
                        <documentation>User ID to give voucher points to</documentation>
                    </annotation>
                </element>
                <element name="voucherOrganizationId" type="string" minOccurs="0" maxOccurs="1">
                    <annotation>
                        <documentation>The voucher organization ID</documentation>
                    </annotation>
                </element>
                <element name="includeInactive" type="boolean" minOccurs="0" maxOccurs="1">
                    <annotation>
                        <documentation>Also return not yet active vouchers?</documentation>
                    </annotation>
                </element>
            </sequence>
            <attribute name="language" type="string" use="optional">
                <annotation>
                    <documentation>ISO 639 alpha-2 or alpha-3 language code. Optional, default is en</documentation>
                </annotation>
            </attribute>
        </complexType>
    </element>

    <element name="VoucherListResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:VoucherListSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:VoucherListError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="VoucherListSuccess">
        <sequence>
            <element name="vouchers" type="tns:Voucher" minOccurs="0" maxOccurs="unbounded" />
        </sequence>
        <attribute name="total" type="long" use="required" />
    </complexType>

    <complexType name="VoucherListError">
		<attribute name="errorCode" type="tns:VoucherListErrorCode" use="required" />
    </complexType>

    <simpleType name="VoucherListErrorCode">
		<annotation>
			<documentation>Voucher List Error codes</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.unknown.user">
				<annotation>
					<documentation>No such user</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.unknown.voucher.organization">
				<annotation>
					<documentation>No such voucher organization</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.permission.denied">
				<annotation>
					<documentation>Application not allowed to list vouchers for this organization</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.unexpected">
				<annotation>
					<documentation>Something unexpected happened</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- Vouchers: list redeemed -->

