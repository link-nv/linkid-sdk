    <!-- Voucher organizations: history -->

    <element name="VoucherOrganizationHistoryRequest">
        <complexType>
            <sequence>
                <element name="voucherOrganizationIds" type="string" minOccurs="0" maxOccurs="unbounded">
                    <annotation>
                        <documentation>The IDs of the requested voucher organization</documentation>
                    </annotation>
                </element>
                <element name="eventTypeFilter" type="tns:VoucherEventTypeFilter" minOccurs="0" maxOccurs="1">
                    <annotation>
                        <documentation>Optional voucher event filter</documentation>
                    </annotation>
                </element>
                <element name="userFilter" type="tns:UserFilter" minOccurs="0" maxOccurs="1">
                    <annotation>
                        <documentation>Optional user filter</documentation>
                    </annotation>
                </element>
                <element name="applicationFilter" type="tns:ApplicationFilter" minOccurs="0" maxOccurs="1">
                    <annotation>
                        <documentation>Optional application filter</documentation>
                    </annotation>
                </element>
                <element name="dateFilter" type="tns:ReportDateFilter" minOccurs="0" maxOccurs="1">
                    <annotation>
                        <documentation>Optional date filter</documentation>
                    </annotation>
                </element>
                <element name="pageFilter" type="tns:ReportPageFilter" minOccurs="0" maxOccurs="1">
