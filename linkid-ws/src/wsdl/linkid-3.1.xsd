<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="urn:net:lin-k:linkid:3.1:core" xmlns:tns="urn:net:lin-k:linkid:3.1:core"
        elementFormDefault="qualified">

	<annotation>
		<documentation>
			Document identifier: linkid
			Revision history:
			V3.0 (October 7, 2015):
			First version that merged all linkID specific non standaard WS's
            V3.1 (November 18, 2015):
            QRCodeInfo.targetBlank support dropped, favorites configuration support, payment refund support, extended wallet report
            V3.7 (November 25, 2015):
            WalletInfoReport: returns localized wallet organization, WalletReport: returns localized application, returns payment description if applicable
		</documentation>
	</annotation>

    <!-- **************************************************************************************************************************************************
         *                                                                  Authentication                                                                *
         **************************************************************************************************************************************************  -->

    <!-- Auth: Start request -->

    <element name="AuthStartRequest">
        <annotation>
            <documentation>Start a linkID authentication request</documentation>
        </annotation>
        <complexType>
            <sequence>
                <any namespace='##any' processContents='lax' minOccurs='1' maxOccurs='1'>
                    <annotation>
                        <documentation>The authentication request, e.g. a SAML2 request</documentation>
                    </annotation>
                </any>
            </sequence>
            <attribute name="language" type="string" use="optional">
                <annotation>
                    <documentation>ISO 639 alpha-2 or alpha-3 language code. Optional, default is en</documentation>
                </annotation>
            </attribute>
            <attribute name="userAgent" type="string" use="optional">
                <annotation>
                    <documentation>Optional user agent string, for adding e.g. callback params to the QR code URL, android chrome URL needs to be http://linkidmauthurl/MAUTH/2/zUC8oA/eA==, ...</documentation>
                </annotation>
            </attribute>
        </complexType>
    </element>

    <element name="AuthStartResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:AuthStartSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:AuthStartError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="AuthStartSuccess">
        <sequence>
            <element name="sessionId" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The authentication session ID, use this in the poll operation</documentation>
                </annotation>
            </element>
            <element name="qrCodeInfo" type="tns:QRCodeInfo" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>linkID QR Code Information</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="AuthStartError">
        <sequence>
            <element name="error" type="tns:AuthStartErrorCode" minOccurs="1" maxOccurs="1" />
            <element name="info" type="string" minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>

    <simpleType name="AuthStartErrorCode">
		<annotation>
			<documentation>Error codes for a Start request</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.request.invalid">
				<annotation>
					<documentation>The authentication request was invalid</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- Auth: Poll request -->

    <element name="AuthPollRequest">
        <annotation>
            <documentation>Poll the linkID authentication</documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="sessionId" type="string" minOccurs="1" maxOccurs="1">
                    <annotation>
                        <documentation>The session ID of the linkID authentication</documentation>
                    </annotation>
                </element>
            </sequence>
            <attribute name="language" type="string" use="optional">
                <annotation>
                    <documentation>ISO 639 alpha-2 or alpha-3 language code. Optional, default is en</documentation>
                </annotation>
            </attribute>
        </complexType>
    </element>

    <element name="AuthPollResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:AuthPollSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:AuthPollError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="AuthPollSuccess">
        <sequence>
            <element name="authenticationState" type="tns:AuthAuthenticationState" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The authentication state</documentation>
                </annotation>
            </element>
            <element name="paymentState" type="tns:PaymentStatusType" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>Optional payment state</documentation>
                </annotation>
            </element>
            <element name="paymentMenuURL" type="string" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>Payment menu URL case user has authenticated for a payment and chosen add a new payment method ( linkid.state.authenticated.payment.add )</documentation>
                </annotation>
            </element>
            <element name="authenticationResponse" type="tns:AuthAuthenticationResponse" minOccurs="0" maxOccurs="1" />
        </sequence>
    </complexType>

    <complexType name="AuthAuthenticationResponse">
        <sequence>
            <any namespace='##any' processContents='lax' minOccurs='0' maxOccurs='1'>
                <annotation>
                    <documentation>The authentication response if completed, e.g. a SAML2 response</documentation>
                </annotation>
            </any>
        </sequence>
    </complexType>

    <simpleType name="AuthAuthenticationState">
		<annotation>
			<documentation>Authentication states</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="linkid.state.started">
				<annotation>
					<documentation>The linkID authentication has started but the client has not yet retrieved the session</documentation>
				</annotation>
			</enumeration>
			<enumeration value="linkid.state.retrieved">
				<annotation>
					<documentation>The linkID client has retrieved the authentication session</documentation>
				</annotation>
			</enumeration>
			<enumeration value="linkid.state.authenticated">
				<annotation>
					<documentation>Authentication was successful</documentation>
				</annotation>
			</enumeration>
			<enumeration value="linkid.state.expired">
				<annotation>
					<documentation>The authentication session has expired</documentation>
				</annotation>
			</enumeration>
			<enumeration value="linkid.state.failed">
				<annotation>
					<documentation>The linkID authentication has failed</documentation>
				</annotation>
			</enumeration>
			<enumeration value="linkid.state.payment.add">
				<annotation>
					<documentation>The linkID authentication was ok, user did not have payment tokens and started the add new payment method flow</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <complexType name="AuthPollError">
        <sequence>
            <element name="error" type="tns:AuthPollErrorCode" minOccurs="1" maxOccurs="1" />
            <element name="info" type="string" minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>

    <simpleType name="AuthPollErrorCode">
		<annotation>
			<documentation>Error codes for a Poll request</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.response.invalid.sessionId">
				<annotation>
					<documentation>No linkID authentication was found with this sessionId</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- Auth: Cancel request -->

    <element name="AuthCancelRequest">
        <annotation>
            <documentation>Cancel a linkID authentication request</documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="sessionId" type="string" minOccurs="1" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <element name="AuthCancelResponse">
        <complexType>
            <sequence>
                <element name="error" type="tns:AuthCancelError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="AuthCancelError">
        <sequence>
            <element name="error" type="tns:AuthCancelErrorCode" minOccurs="1" maxOccurs="1" />
            <element name="info" type="string" minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>

    <simpleType name="AuthCancelErrorCode">
		<annotation>
			<documentation>Error codes for a Cancel request</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.invalid.sessionId">
				<annotation>
					<documentation>No linkID authentication session was found with this sessionId</documentation>
				</annotation>
			</enumeration>
            <enumeration value="error.permission.denied">
				<annotation>
					<documentation>Application is not allowed to cancel this linkID authentication session</documentation>
				</annotation>
            </enumeration>
			<enumeration value="error.unexpected">
				<annotation>
					<documentation>Something went wrong trying to cancel the linkID authentication session</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- **************************************************************************************************************************************************
         *                                                                  Callback                                                                      *
         **************************************************************************************************************************************************  -->

    <!-- Callback: Pull request -->

    <element name="CallbackPullRequest">
        <annotation>
            <documentation>Fetch an authentication response for a linkID callback</documentation>
        </annotation>
        <complexType>
            <sequence>
                <element name="sessionId" type="string" minOccurs="1" maxOccurs="1">
                    <annotation>
                        <documentation>The session ID for the callback authentication response, returned in the redirect</documentation>
                    </annotation>
                </element>
            </sequence>
        </complexType>
    </element>

    <element name="CallbackPullResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:CallbackPullSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:CallbackPullError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="CallbackPullSuccess">
        <sequence>
            <any namespace='##any' processContents='lax' minOccurs='1' maxOccurs='1'>
                <annotation>
                    <documentation>The authentication response, e.g. a SAML2 response</documentation>
                </annotation>
            </any>
        </sequence>
    </complexType>

    <complexType name="CallbackPullError">
        <sequence>
            <element name="error" type="tns:CallbackPullErrorCode" minOccurs="1" maxOccurs="1" />
            <element name="info" type="string" minOccurs="1" maxOccurs="1" />
        </sequence>
    </complexType>

    <simpleType name="CallbackPullErrorCode">
		<annotation>
			<documentation>Error codes for a Callback pull request</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.response.invalid.sessionId">
				<annotation>
					<documentation>No authentication response found with this sessionId</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.unexpected">
				<annotation>
					<documentation>Something went wrong trying to pull the callback authentication response</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- **************************************************************************************************************************************************
         *                                                                  Configuration                                                                 *
         **************************************************************************************************************************************************  -->

    <!-- Configuration: Theme request -->

    <element name="ConfigThemesRequest">
		<complexType>
            <attribute name="applicationName" type="string" use="required" />
		</complexType>
	</element>

	<element name="ConfigThemesResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:ConfigThemesSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:ConfigThemesError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="ConfigThemesSuccess">
        <sequence>
            <element name="themes" type="tns:ConfigThemes" minOccurs="0" maxOccurs="unbounded">
                <annotation>
                    <documentation>The theme(s)</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="ConfigThemes">
        <sequence>
            <element name="name" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The theme name</documentation>
                </annotation>
            </element>
            <element name="defaultTheme" type="boolean" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Default theme?</documentation>
                </annotation>
            </element>
            <element name="logo" type="tns:ConfigLocalizedImages" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>The application logo, shown in the manage linkID section</documentation>
                </annotation>
            </element>
            <element name="authLogo" type="tns:ConfigLocalizedImages" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>The application logo, shown during an authentication/payment</documentation>
                </annotation>
            </element>
            <element name="background" type="tns:ConfigLocalizedImages" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>The application background, shown in phone size devices</documentation>
                </annotation>
            </element>
            <element name="tabletBackground" type="tns:ConfigLocalizedImages" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>The application background, shown in tablet size devices</documentation>
                </annotation>
            </element>
            <element name="alternativeBackground" type="tns:ConfigLocalizedImages" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>The application background, shown in alternative size devices</documentation>
                </annotation>
            </element>
            <element name="backgroundColor" type="string" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>The application background color (#xxxxxx), shown if no background images are defined, or they are being loaded</documentation>
                </annotation>
            </element>
            <element name="textColor" type="string" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>The application text color (#xxxxxx), e.g. for the pinpad, ...</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="ConfigLocalizedImages">
        <sequence>
            <element name="images" type="tns:ConfigLocalizedImage" minOccurs="1" maxOccurs="unbounded">
                <annotation>
                    <documentation>The localized images</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="ConfigLocalizedImage">
        <sequence>
            <element name="url" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The image url</documentation>
                </annotation>
            </element>
            <element name="language" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>Locale of the image</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

	<complexType name="ConfigThemesError">
        <attribute name="errorCode" type="tns:ConfigThemesErrorCode" use="required" />
    </complexType>

    <simpleType name="ConfigThemesErrorCode">
		<annotation>
			<documentation>Error codes</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.unknown.application">
				<annotation>
					<documentation>No application with that name is connected to linkID</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- Configuration: Localization request -->

    <element name="ConfigLocalizationRequest">
		<complexType>
            <sequence>
                <element name="key" type="string" minOccurs="1" maxOccurs="unbounded">
                    <annotation>
                        <documentation>linkID key to get localization for, e.g. a wallet organization ID, wallet coin ID,...</documentation>
                    </annotation>
                </element>
            </sequence>
		</complexType>
	</element>

	<element name="ConfigLocalizationResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:ConfigLocalizationSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:ConfigLocalizationError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="ConfigLocalizationSuccess">
        <sequence>
            <element name="localization" type="tns:ConfigLocalization" minOccurs="0" maxOccurs="unbounded">
                <annotation>
                    <documentation>The localization key(s)</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="ConfigLocalization">
        <sequence>
            <element name="key" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>The key</documentation>
                </annotation>
            </element>
            <element name="values" type="tns:ConfigLocalizationValue" minOccurs="1" maxOccurs="unbounded">
                <annotation>
                    <documentation>Localization values</documentation>
                </annotation>
            </element>
        </sequence>
        <attribute name="type" type="tns:ConfigLocalizationKeyType" use="required" />
    </complexType>

    <simpleType name="ConfigLocalizationKeyType">
		<annotation>
			<documentation>Localization key type</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="localization.key.friendly" />
            <enumeration value="localization.key.friendly.multiple" />
            <enumeration value="localization.key.description" />
		</restriction>
	</simpleType>

    <complexType name="ConfigLocalizationValue">
        <sequence>
            <element name="languageCode" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>ISO 639‑1 code of the language</documentation>
                </annotation>
            </element>
            <element name="localized" type="string" minOccurs="1" maxOccurs="1">
                <annotation>
                    <documentation>localized value</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

	<complexType name="ConfigLocalizationError">
        <attribute name="errorCode" type="tns:ConfigLocalizationErrorCode" use="required" />
    </complexType>

    <simpleType name="ConfigLocalizationErrorCode">
		<annotation>
			<documentation>Error codes</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.unexpected">
				<annotation>
					<documentation>Something unexpected occurred</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- **************************************************************************************************************************************************
         *                                                                  Payments                                                                      *
         **************************************************************************************************************************************************  -->

    <!-- Payment: Status request -->

    <element name="PaymentStatusRequest">
		<complexType>
            <attribute name="orderReference" type="string" use="required" />
		</complexType>
	</element>

    <element name="PaymentStatusResponse">
        <complexType>
            <sequence>
                <element name="success" type="tns:PaymentStatusSuccess" minOccurs="0" maxOccurs="1" />
                <element name="error" type="tns:PaymentStatusError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

    <complexType name="PaymentStatusSuccess">
        <sequence>
            <element name="orderReference" minOccurs="1" maxOccurs="1" type="string">
                <annotation>
                    <documentation>The payment order reference</documentation>
                </annotation>
            </element>
            <element name="userId" minOccurs="1" maxOccurs="1" type="string">
                <annotation>
                    <documentation>The linkID user's ID that made this order</documentation>
                </annotation>
            </element>
            <element name="paymentStatus" minOccurs="1" maxOccurs="1" type="tns:PaymentStatusType">
                <annotation>
                    <documentation>Order payment status</documentation>
                </annotation>
            </element>
            <element name="authorized" minOccurs="1" maxOccurs="1" type="boolean">
                <annotation>
                    <documentation>Order authorized?</documentation>
                </annotation>
            </element>
            <element name="captured" minOccurs="1" maxOccurs="1" type="boolean">
                <annotation>
                    <documentation>Order captured? Only makes sense for orders with Docdata transactions for a merchant with automatic capture disabled</documentation>
                </annotation>
            </element>
            <element name="amountPayed" minOccurs="1" maxOccurs="1" type="double">
                <annotation>
                    <documentation>How much of the order was payed (in cents)? This only makes sense if partial payments were allowed for this order</documentation>
                </annotation>
            </element>
            <element name="amount" minOccurs="1" maxOccurs="1" type="double">
                <annotation>
                    <documentation>Amount that was intended to be payed, i.e. what was specified in the linkID payment context</documentation>
                </annotation>
            </element>
            <element name="refundAmount" minOccurs="1" maxOccurs="1" type="double">
                <annotation>
                    <documentation>Amount that has been refunded</documentation>
                </annotation>
            </element>
            <element name="currency" minOccurs="0" maxOccurs="1" type="tns:Currency">
                <annotation>
                    <documentation>Optional payment amount currency</documentation>
                </annotation>
            </element>
            <element name="walletCoin" type="string" minOccurs="0" maxOccurs="1">
                <annotation>
                    <documentation>Optional payment amount wallet coin</documentation>
                </annotation>
            </element>
            <element name="description" minOccurs="0" maxOccurs="1" type="string">
                <annotation>
                    <documentation>Optional payment description as is passed on in the linkID payment context</documentation>
                </annotation>
            </element>
            <element name="profile" minOccurs="0" maxOccurs="1" type="string">
                <annotation>
                    <documentation>Optional linkID payment profile used</documentation>
                </annotation>
            </element>
            <element name="created" minOccurs="1" maxOccurs="1" type="dateTime">
                <annotation>
                    <documentation>When was the order created</documentation>
                </annotation>
            </element>
            <element name="mandateReference" minOccurs="0" maxOccurs="1" type="string">
                <annotation>
                    <documentation>Optional mandate reference</documentation>
                </annotation>
            </element>
            <element name="paymentDetails" minOccurs="1" maxOccurs="1" type="tns:PaymentDetails">
                <annotation>
                    <documentation>Order payment details</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="PaymentDetails">
        <sequence>
            <element name="paymentTransactions" minOccurs="0" maxOccurs="unbounded" type="tns:PaymentTransaction">
                <annotation>
                    <documentation>Regular payment transactions, VISA, MASTERCARD, SEPA, ...</documentation>
                </annotation>
            </element>
            <element name="walletTransactions" minOccurs="0" maxOccurs="unbounded" type="tns:WalletTransaction">
                <annotation>
                    <documentation>Wallet payment transactions</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>

    <complexType name="PaymentStatusError">
        <attribute name="errorCode" type="tns:PaymentStatusErrorCode" use="required" />
    </complexType>

    <simpleType name="PaymentStatusErrorCode">
		<annotation>
			<documentation>Payment status Error codes</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.not.found">
				<annotation>
					<documentation>The payment order was not found.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.multiple.orders.found">
				<annotation>
					<documentation>Multiple payment orders exist for this payment order reference, this should not happend. Contact someone from linkID.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.unexpected">
				<annotation>
					<documentation>Something unexpected happened.</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance.</documentation>
				</annotation>
			</enumeration>
		</restriction>
	</simpleType>

    <!-- Payment: Capture request -->

    <element name="PaymentCaptureRequest">
		<complexType>
            <attribute name="orderReference" type="string" use="required" />
		</complexType>
	</element>

	<element name="PaymentCaptureResponse">
        <complexType>
            <sequence>
                <element name="error" type="tns:PaymentCaptureError" minOccurs="0" maxOccurs="1" />
            </sequence>
        </complexType>
    </element>

	<complexType name="PaymentCaptureError">
        <attribute name="errorCode" type="tns:PaymentCaptureErrorCode" use="required" />
    </complexType>

    <simpleType name="PaymentCaptureErrorCode">
		<annotation>
			<documentation>Payment Capture Error codes</documentation>
		</annotation>
		<restriction base="string">
			<enumeration value="error.capture.unknown">
				<annotation>
					<documentation>No payment transaction found with that orderReference</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.capture.token.not.found">
				<annotation>
					<documentation>Could not capture this payment because the payment token has been removed</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.capture.failed">
				<annotation>
					<documentation>Could not capture this payment for an unexpected reason</documentation>
				</annotation>
			</enumeration>
			<enumeration value="error.maintenance">
				<annotation>
					<documentation>linkID is down for maintenance</documentation>
				</annotation>
			</enumeration>
