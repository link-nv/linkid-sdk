                    <documentation>Optional error message containing additional info</documentation>
                </annotation>
            </element>
        </sequence>
        <attribute name="errorCode" type="tns:LTQRPushErrorCode" use="required" />
    </complexType>

    <complexType name="LTQRChangeSuccess">
